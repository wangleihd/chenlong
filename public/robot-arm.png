<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#F5F5F5"/>
  <!-- 机器人底座 -->
  <rect x="200" y="450" width="400" height="100" rx="20" fill="#424242"/>
  <!-- 机器人主体 -->
  <rect x="350" y="250" width="100" height="200" rx="10" fill="#616161" stroke="#424242" stroke-width="5"/>
  <!-- 电机 -->
  <circle cx="400" cy="300" r="15" fill="#FFC107"/>
  <circle cx="400" cy="400" r="15" fill="#FFC107"/>
  <!-- 左侧机械臂 -->
  <g transform="translate(350, 300)">
    <!-- 大臂 -->
    <rect x="-15" y="-15" width="30" height="100" fill="#757575" stroke="#424242" stroke-width="3" transform="rotate(-45, 0, 0)"/>
    <!-- 小臂 -->
    <rect x="-10" y="70" width="20" height="80" fill="#757575" stroke="#424242" stroke-width="3" transform="rotate(45, 0, 80)"/>
    <!-- 机械爪 -->
    <path d="M0,140 L10,160 L15,155 L5,150 Z" fill="#424242"/>
    <path d="M0,140 L-10,160 L-15,155 L-5,150 Z" fill="#424242"/>
  </g>
  <!-- 右侧机械臂 -->
  <g transform="translate(450, 300)">
    <!-- 大臂 -->
    <rect x="-15" y="-15" width="30" height="100" fill="#757575" stroke="#424242" stroke-width="3" transform="rotate(45, 0, 0)"/>
    <!-- 小臂 -->
    <rect x="-10" y="70" width="20" height="80" fill="#757575" stroke="#424242" stroke-width="3" transform="rotate(-45, 0, 80)"/>
    <!-- 机械爪 -->
    <path d="M0,140 L10,160 L15,155 L5,150 Z" fill="#424242"/>
    <path d="M0,140 L-10,160 L-15,155 L-5,150 Z" fill="#424242"/>
  </g>
  <!-- 顶部传感器 -->
  <rect x="385" y="200" width="30" height="50" rx="5" fill="#616161" stroke="#424242" stroke-width="3"/>
  <circle cx="400" cy="225" r="8" fill="#2196F3"/>
  <!-- 轮子 -->
  <circle cx="250" cy="500" r="30" fill="#212121"/>
  <circle cx="550" cy="500" r="30" fill="#212121"/>
  <!-- 黄色装饰 -->
  <rect x="350" y="480" width="20" height="20" fill="#FFC107"/>
  <rect x="430" y="480" width="20" height="20" fill="#FFC107"/>
</svg>