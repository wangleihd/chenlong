<svg xmlns="http://www.w3.org/2000/svg" width="800" height="800" viewBox="0 0 800 800">
  <rect width="800" height="800" fill="#E8F5E8"/>
  <!-- 网球场 -->
  <rect x="150" y="100" width="500" height="600" fill="#1976D2" stroke="#FFFFFF" stroke-width="8"/>
  <!-- 网球场线 -->
  <rect x="150" y="250" width="500" height="300" fill="none" stroke="#FFFFFF" stroke-width="4"/>
  <rect x="300" y="100" width="200" height="600" fill="none" stroke="#FFFFFF" stroke-width="4"/>
  <rect x="300" y="250" width="200" height="300" fill="none" stroke="#FFFFFF" stroke-width="2" stroke-dasharray="10,10"/>
  <!-- 网 -->
  <rect x="150" y="396" width="500" height="8" fill="#424242"/>
  <rect x="120" y="380" width="12" height="40" fill="#424242"/>
  <rect x="668" y="380" width="12" height="40" fill="#424242"/>
  <!-- 网球 -->
  <g fill="#FFEB3B">
    <circle cx="300" cy="200" r="15"/>
    <circle cx="500" cy="200" r="15"/>
    <circle cx="350" cy="250" r="15"/>
    <circle cx="450" cy="250" r="15"/>
    <circle cx="400" cy="300" r="15"/>
    <circle cx="350" cy="350" r="15"/>
    <circle cx="450" cy="350" r="15"/>
    <circle cx="300" cy="400" r="15"/>
    <circle cx="500" cy="400" r="15"/>
    <circle cx="350" cy="450" r="15"/>
    <circle cx="450" cy="450" r="15"/>
    <circle cx="400" cy="500" r="15"/>
    <circle cx="350" cy="550" r="15"/>
    <circle cx="450" cy="550" r="15"/>
  </g>
  <!-- 网球筐 -->
  <g fill="#9E9E9E">
    <rect x="40" y="200" width="100" height="150" rx="10" stroke="#757575" stroke-width="4"/>
    <rect x="660" y="200" width="100" height="150" rx="10" stroke="#757575" stroke-width="4"/>
    <rect x="40" y="500" width="100" height="150" rx="10" stroke="#757575" stroke-width="4"/>
    <rect x="660" y="500" width="100" height="150" rx="10" stroke="#757575" stroke-width="4"/>
  </g>
  <!-- 网球筐中的网球 -->
  <g fill="#FFEB3B">
    <circle cx="60" cy="250" r="10"/>
    <circle cx="90" cy="230" r="10"/>
    <circle cx="70" cy="270" r="10"/>
    <circle cx="680" cy="250" r="10"/>
    <circle cx="710" cy="230" r="10"/>
    <circle cx="690" cy="270" r="10"/>
    <circle cx="60" cy="550" r="10"/>
    <circle cx="90" cy="530" r="10"/>
    <circle cx="70" cy="570" r="10"/>
    <circle cx="680" cy="550" r="10"/>
    <circle cx="710" cy="530" r="10"/>
    <circle cx="690" cy="570" r="10"/>
  </g>
  <!-- 机器人1 -->
  <g transform="translate(300, 150)">
    <!-- 底盘 -->
    <ellipse cx="0" cy="30" rx="35" ry="20" fill="#424242"/>
    <circle cx="-25" cy="35" r="8" fill="#212121"/>
    <circle cx="25" cy="35" r="8" fill="#212121"/>
    <!-- 机身 -->
    <rect x="-15" y="-20" width="30" height="50" fill="#757575" stroke="#424242" stroke-width="3"/>
    <!-- 机械臂 -->
    <rect x="-8" y="-30" width="16" height="20" fill="#616161" stroke="#424242" stroke-width="2"/>
    <rect x="-4" y="-45" width="8" height="20" fill="#616161" stroke="#424242" stroke-width="2" transform="rotate(-30, 0, -35)"/>
    <!-- 机械爪 -->
    <path d="M0,-50 L5,-60 L10,-55 L5,-50 Z" fill="#424242"/>
    <path d="M0,-50 L-5,-60 L-10,-55 L-5,-50 Z" fill="#424242"/>
    <!-- 摄像头 -->
    <circle cx="0" cy="-10" r="8" fill="#212121"/>
    <!-- 抓取的网球 -->
    <circle cx="0" cy="-60" r="12" fill="#FFEB3B"/>
  </g>
  <!-- 机器人2 -->
  <g transform="translate(500, 650)">
    <!-- 底盘 -->
    <ellipse cx="0" cy="30" rx="35" ry="20" fill="#424242"/>
    <circle cx="-25" cy="35" r="8" fill="#212121"/>
    <circle cx="25" cy="35" r="8" fill="#212121"/>
    <!-- 机身 -->
    <rect x="-15" y="-20" width="30" height="50" fill="#757575" stroke="#424242" stroke-width="3"/>
    <!-- 机械臂 -->
    <rect x="-8" y="-30" width="16" height="20" fill="#616161" stroke="#424242" stroke-width="2"/>
    <rect x="-4" y="-45" width="8" height="20" fill="#616161" stroke="#424242" stroke-width="2" transform="rotate(30, 0, -35)"/>
    <!-- 机械爪 -->
    <path d="M0,-50 L5,-60 L10,-55 L5,-50 Z" fill="#424242"/>
    <path d="M0,-50 L-5,-60 L-10,-55 L-5,-50 Z" fill="#424242"/>
    <!-- 摄像头 -->
    <circle cx="0" cy="-10" r="8" fill="#212121"/>
    <!-- 抓取的网球 -->
    <circle cx="0" cy="-60" r="12" fill="#FFEB3B"/>
  </g>
</svg>